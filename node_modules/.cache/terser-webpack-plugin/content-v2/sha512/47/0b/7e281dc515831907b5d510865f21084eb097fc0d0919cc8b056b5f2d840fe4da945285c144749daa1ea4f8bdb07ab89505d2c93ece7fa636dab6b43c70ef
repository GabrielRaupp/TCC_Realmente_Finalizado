ais horrios so visveis\n\n  useEffect(() => {\n    axios.get('/contas', { withCredentials: true })\n      .then(response => {\n        setData(response.data);\n        setLoading(false);\n      })\n      .catch(error => {\n        console.error('Erro ao carregar dados:', error);\n        setLoading(false);\n      });\n  }, []);\n\n  // Funo para alternar a visibilidade dos horrios de um usurio\n  const toggleHorarios = (userId) => {\n    setVisibleHorarios(prevState => ({\n      ...prevState,\n      [userId]: !prevState[userId] // Alterna a visibilidade para o usurio especfico\n    }));\n  };\n\n  if (loading) {\n    return <p>Carregando...</p>;\n  }\n\n  return (\n    <div className={styles.contas_container}>\n      \n      <div>\n        <h2>Usurios:</h2>\n        <div className={styles.contas_card}>\n          <ul>\n            {data.users.map(user => (\n              <li key={user._id}>\n                <p><span>Nome:</span> {user.username}</p>\n                <p><span>Email:</span> {user.email}</p>\n                <p><span>Campus:</span> {user.campus}</p>\n                <p><span>Data de criao:</span> {new Date(user.createdAt).toLocaleDateString()}</p>\n                {/* Boto para alternar a visibilidade dos horrios */}\n                <button onClick={() => toggleHorarios(user._id)}>\n                  {visibleHorarios[user._id] ? 'Ocultar horrios' : 'Mostrar horrios'}\n                </button>\n\n                {/* Exibe os horrios somente se visvel */}\n                {visibleHorarios[user._id] && (\n                  <div>\n                    <h3>Horrios:</h3>\n                    <ul>\n                      {data.horarios.filter(horario => horario.user._id === user._id).map(horario => (\n                        <li key={horario._id}>\n                          <p><span>Nome:</span> {horario.name}</p>\n                          <p><span>Categoria:</span> {horario.category}</p>\n                          <p><span>Usurio:</span> {horario.user.userna